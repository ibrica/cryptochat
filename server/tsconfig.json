
{
    "compilerOptions": {
      "target": "es5",
      "module": "esnext",
      "strict": true,
      "importHelpers": true,
      "moduleResolution": "node",
      "experimentalDecorators": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "noImplicitAny": false,
      "sourceMap": true,
      "inlineSources": true,
      "outDir": "dist",
      "baseUrl": ".",
      "paths": {
        "*": [
          "node_modules/*",
          "src/types/*"
        ]
      },
      "types": [
         "lodash",
         "node",
          "webpack-env"
      ],
      "typeRoots": [
         "./node_modules/@types"
      ],
    },
    "exclude": [
      "node_modules/*"
    ],
    "lib": [
      "esnext"
    ],
    "include": [
    ],
  }